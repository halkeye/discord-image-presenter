    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.4",
    "require-extension-hooks": "^0.3.3",
