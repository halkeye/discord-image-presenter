    "test:e2e": "cross-env TEST=e2e ava ./test/e2e/**/*"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "^5.0.0-1643365392.dae6516",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.19.3",
    "discord.js": "^13.6.0",
    "express": "^4.17.2",
    "nuxt": "^2.15.8",
    "nuxt-socket-io": "^2.0.3",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
