    "@nuxtjs/sentry": "^5.1.7",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.23.0",
    "discord.js": "^13.6.0",
    "express": "^4.17.2",
    "node-fetch": "^3.2.0",
    "nuxt": "^2.15.8",
